{
	"name": "TextboxTemplate",
	"visualStyles": {
		"textbox": {
			"*": {
				"*": [{
					"fontSize": 40,
					"fontFamily": "Calibri",
					"wordWrap": true,
					"color": { "solid": { "color": "#FFFFFF"} }
				}],
				"general": [{
					"responsive": true,
					"keepLayerOrder": true
				}],
				"border": [{
					"color": { "solid": { "color": "#808080"}},
					"show": true,
					"radius": 0
				}],
				"title": [{
					"show": true,
					"fontColor": { "solid": { "color": "#000000" } },
					"background": { "solid": { "color": "#FFFFFF" } },
					"alignment": "center"
				}],
				"background": [{
					"show": true,
					"color": { "solid": { "color": "#DDDDDD"}},
					"transparency": 0
				}],
				"visualHeader": [{
					"show": true
				}]
			}
		}
	}
}
